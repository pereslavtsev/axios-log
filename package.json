{
  "name": "axios-log",
  "version": "0.0.1",
  "description": "Easy way to log all Axios calls",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib dist && webpack",
    "dev": "webpack-dev-server --no-inline",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "release": "standard-version --no-verify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pereslavtsev/axios-log.git"
  },
  "keywords": [
    "axios",
    "http",
    "log",
    "logger"
  ],
  "author": {
    "name": "Vitaly Pereslavtsev",
    "email": "dev@pereslavtsev.com",
    "url": "https://www.pereslavtsev.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/pereslavtsev/axios-log/issues"
  },
  "homepage": "https://github.com/pereslavtsev/axios-log#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "axios": "^0.19.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": ">=10",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "axios": "*"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
